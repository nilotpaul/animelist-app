{
  "name": "vite-tailwind-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npx tailwindcss -i ./src/globals.css -o ./dist/output.css --watch\"",
    "build": "tsc && vite build && npx tailwindcss -o ./dist/assets/build.css --minify",
    "prod": "npm install && tsc && vite build && npx tailwindcss -o ./dist/assets/build.css --minify",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.4",
